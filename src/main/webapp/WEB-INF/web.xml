<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>flightingregister</servlet-name>
        <servlet-class>travel.flightingregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>travel.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLoginServlet</servlet-name>
        <servlet-class>travel.AdminLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bussingregister</servlet-name>
        <servlet-class>travel.bussingregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>trainingregister</servlet-name>
        <servlet-class>travel.trainingregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testbooking</servlet-name>
        <servlet-class>travel.testbooking</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>flightingregister</servlet-name>
        <url-pattern>/flightingregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLoginServlet</servlet-name>
        <url-pattern>/AdminLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bussingregister</servlet-name>
        <url-pattern>/bussingregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>trainingregister</servlet-name>
        <url-pattern>/trainingregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testbooking</servlet-name>
        <url-pattern>/testbooking</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
